<help>
  <name>mris_euler_number</name>
  <synopsis>mris_euler_number [&lt;options&gt;] insurf</synopsis>
  <description>This program computes EulerNumber for a cortical surface</description>
  <arguments>
    <positional>
      <argument>insurf</argument>
      <explanation>input surface file</explanation>
    </positional>
    <required-flagged>
      <intro>None</intro>
    </required-flagged>
    <optional-flagged>
      <intro>-o outfile</intro>
      <explanation>write number of holes to outfile</explanation>
    </optional-flagged>
  </arguments>
  <outputs>
    <output>eulernumber</output>
    <explanation>EulerNumber as text to stdout</explanation>
  </outputs>
	<example>mris_euler_number surface</example>
  <reporting>Report bugs to &lt;freesurfer@nmr.mgh.harvard.edu&gt;</reporting>
</help>

